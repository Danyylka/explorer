{
  "name": "celestiascan",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/@modularcloud/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "web": "turbo run dev --filter=web",
    "docs": "turbo run dev --filter=docs",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "prisma": "^4.14.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.348.0",
    "@aws-sdk/s3-request-presigner": "^3.348.0",
    "@babel/preset-env": "^7.22.4",
    "aws-sdk": "^2.1394.0",
    "aws-sdk-js-codemod": "^0.14.1",
    "esbuild": "0.17.8",
    "react": "^18.2.0",
    "react-toastify": "^9.1.3",
    "tailwind-config": "^0.0.0"
  },
  "packageManager": "npm@8.19.2"
}
