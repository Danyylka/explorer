    "react": "^18.2.0",
    "tailwind-config": "*",
    "tailwindcss": "^3.2.4",
    "tsconfig": "*",
    "tsup": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-icons": "^1.1.1",
